<routes>
    <vType id="car" vClass="passenger" guiShape="passenger"/>
    <vType id="truck" vClass="truck" guiShape="truck"/>

    <route id="routeE1" edges="E3 -E0"/>
    <route id="routeE2" edges="E3 -E1"/>
    <route id="routeE3" edges="E3 -E2"/>

    <route id="routeN1" edges="E1 -E0"/>
    <route id="routeN2" edges="E1 -E2"/>
    <route id="routeN3" edges="E1 -E3"/>

    <route id="routeW1" edges="E0 -E1"/>
    <route id="routeW2" edges="E0 -E2"/>
    <route id="routeW3" edges="E0 -E3"/>

    <route id="routeS1" edges="E2 -E0"/>
    <route id="routeS2" edges="E2 -E1"/>
    <route id="routeS3" edges="E2 -E3"/>
<!--
    <vehicle id="0" type="car" depart="0" departLane="free" color="1,0,0" route="route0"/>
    <vehicle id="1" type="truck" depart="0" departLane="free" color="0,1,0" route="route1"/>
-->

    <!--from the east-->
    <flow id="flowE1" begin="0" end="360" period="exp(0.03)" route="routeE1" type="car"/>
    <flow id="flowE2" begin="0" end="360" period="exp(0.03)" route="routeE2" type="car"/>
    <flow id="flowE3" begin="0" end="360" period="exp(0.03)" route="routeE3" type="car"/>

    <!--from the north-->
    <flow id="flowN1" begin="0" end="360" period="exp(0.03)" route="routeN1" type="car"/>
    <flow id="flowN2" begin="0" end="360" period="exp(0.03)" route="routeN2" type="car"/>
    <flow id="flowN3" begin="0" end="360" period="exp(0.03)" route="routeN3" type="car"/>

    <!--from the west-->
    <flow id="flowW1" begin="0" end="360" period="exp(0.03)" route="routeW1" type="car"/>
    <flow id="flowW2" begin="0" end="360" period="exp(0.03)" route="routeW2" type="car"/>
    <flow id="flowW3" begin="0" end="360" period="exp(0.03)" route="routeW3" type="car"/>

    <!--from the south-->
    <flow id="flowS1" begin="0" end="360" period="exp(0.03)" route="routeS1" type="car"/>
    <flow id="flowS2" begin="0" end="360" period="exp(0.03)" route="routeS2" type="car"/>
    <flow id="flowS3" begin="0" end="360" period="exp(0.03)" route="routeS3" type="car"/>

</routes>