<routes>
    <vType id="car" vClass="passenger" guiShape="passenger"/>
    <vType id="truck" vClass="truck" guiShape="truck"/>
    <vType id="ped" vClass="pedestrian" impatience="0.8" jmDriveAfterRedTime="100" guiShape="pedestrian"/>
    <vType id="jeepney" vClass="bus" guiShape="bus"/>


    <route id="routeE1" edges="E3 -E0"/>
    <route id="routeE2" edges="E3 -E1"/>
    <route id="routeE3" edges="E3 -E2"/>

    <route id="routeN1" edges="E1 -E0"/>
    <route id="routeN2" edges="E1 -E2"/>
    <route id="routeN3" edges="E1 -E3"/>

    <route id="routeW1" edges="E0 -E1"/>
    <route id="routeW2" edges="E0 -E2"/>
    <route id="routeW3" edges="E0 -E3"/>

    <route id="routeS1" edges="E2 -E0"/>
    <route id="routeS2" edges="E2 -E1"/>
    <route id="routeS3" edges="E2 -E3"/>

    <route id="route" edges="E0 -E0"/>
<!--
    <vehicle id="0" type="car" depart="0" departLane="free" color="1,0,0" route="route0"/>
    <vehicle id="1" type="truck" depart="0" departLane="free" color="0,1,0" route="route1"/>
-->

    <!--from the east-->
    <flow id="flowE1" begin="0" end="360" period="exp(0.01)" route="routeE1" type="car"/>
    <flow id="flowE2" begin="0" end="360" period="exp(0.01)" route="routeE2" type="car"/>
    <flow id="flowE3" begin="0" end="360" period="exp(0.01)" route="routeE3" type="car"/>

    <!--from the north-->
    <flow id="flowN1" begin="0" end="360" period="exp(0.01)" route="routeN1" type="car"/>
    <flow id="flowN2" begin="0" end="360" period="exp(0.01)" route="routeN2" type="car"/>
    <flow id="flowN3" begin="0" end="360" period="exp(0.01)" route="routeN3" type="car"/>

    <!--from the west-->
    <flow id="flowW1" begin="0" end="360" period="exp(0.01)" route="routeW1" type="car"/>
    <flow id="flowW2" begin="0" end="360" period="exp(0.01)" route="routeW2" type="car"/>
    <flow id="flowW3" begin="0" end="360" period="exp(0.01)" route="routeW3" type="car"/>

    <!--from the south-->
    <flow id="flowS1" begin="0" end="360" period="exp(0.01)" route="routeS1" type="car"/>
    <flow id="flowS2" begin="0" end="360" period="exp(0.01)" route="routeS2" type="car"/>
    <flow id="flowS3" begin="0" end="360" period="exp(0.01)" route="routeS3" type="car"/>


    <!--
    <personFlow id="pf_0" type="ped" begin="0.00" end="3600.00" personsPerHour="1800">
        <personTrip from="E1" to="-E3"/>
    </personFlow>

    <flow id="jeep_flow" type="jeepney" begin="0" end="3600" number="100" from="E1" to="-E3">
        <stop busStop="jeepStop_0" duration="20"/>
    </flow> -->

   <!-- Jeep flow that stops at the jeep stop -->
    <flow id="jeep_flow" type="jeepney" begin="0" end="360" probability="0.1" from="E3" to="-E0">
        <stop busStop="jeepStop_0" duration="5"/>
        <stop busStop="jeepStop_1" duration="5"/>
    </flow>

    <!-- Persons that use the jeep stop -->
    <personFlow id="pflow_0" begin="0" end="3600" probability="0.1">
        <!-- Trip starts by waiting at the jeep stop -->
        <walk from="E3" to="E3" arrivalPos="55"/>       
        <ride from="E3" to="-E0" modes="public"/>
        <walk from="-E0" to="-E3" arrivalPos="55"/> 
    </personFlow>

</routes>